{
  "name": "highlander",
  "version": "1.0.0",
  "description": "Micro service to handle highlander",
  "repository": "https://erzo.wtf",
  "license": "Erzo",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "npm run build && node -r module-alias/register ./dist/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "cross-env ENV_FILE=test ts-node -r ./env -r tsconfig-paths/register ./spec/start.ts",
    "testgit": "ts-node -r tsconfig-paths/register ./spec/start.ts",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "changelog-init": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "changelog-git": "npm run changelog && git add CHANGELOG.md && git commit -m 'docs(changelog): updated' && git push origin master"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "precise-commit"
    }
  },
  "hooks": {
    "pre-commit": [
      {
        "message": "You’ve got leftover conflict markers",
        "regex": "/^[<>|=]{4,}/m"
      },
      {
        "filter": "\\.js$",
        "message": "You’ve got leftover `console.log`",
        "regex": "console\\.log"
      },
      {
        "message": "You have unfinished devs",
        "nonBlocking": "true",
        "regex": "(?:FIXME|TODO)"
      }
    ]
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/cors": "^2.8.6",
    "@types/express-session": "^1.15.16",
    "@types/request": "^2.48.3",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "changelog": "^1.4.2",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "form-data": "^3.0.0",
    "global": "^4.4.0",
    "http-status-codes": "^1.3.2",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jasmine-node": "^3.0.0",
    "kaop": "^4.4.2",
    "kaop-ts": "^4.3.0",
    "log4js": "^6.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "sequelize": "^5.21.2",
    "sequelize-history": "^1.3.2",
    "sequelize-typescript": "^1.0.0",
    "swagger-express-ts": "^1.0.1",
    "swagger-ui-dist": "^3.24.3",
    "typescript-rest": "^2.2.2",
    "uuid": "^3.3.3",
    "uuidv4": "^6.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.5.0",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.8",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.1",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.0.10",
    "jasmine": "^3.5.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
